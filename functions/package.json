{
  "name": "functions",
  "scripts": {
    "lint": "eslint --ext .js,.ts .",
    "build": "tsc",
    "build:watch": "tsc --watch",
    "serve": "npm run build && firebase emulators:start --only functions",
    "shell": "npm run build && firebase functions:shell",
    "start": "npm run shell",
    "deploy": "firebase deploy --only functions",
    "logs": "firebase functions:log",
    "test:triggers": "npx ts-node src/testTriggersCLI.ts",
    "test:triggers:coverage": "npx ts-node src/testTriggersCLI.ts --coverage",
    "test:triggers:deployed": "firebase functions:call runTriggerTests",
    "test:triggers:deployed:coverage": "firebase functions:call checkTriggerCoverage",
    "test:triggers:dashboard": "firebase functions:call getTestResultsDashboard",
    "check:maintenance": "npx ts-node src/checkTestMaintenance.ts"
  },
  "engines": {
    "node": "20"
  },
  "main": "lib/index.js",
  "dependencies": {
    "@sendgrid/mail": "^8.1.5",
    "@types/cors": "^2.8.19",
    "busboy": "^1.6.0",
    "cheerio": "^1.1.2",
    "compromise": "^14.14.4",
    "cors": "^2.8.5",
    "dotenv": "^17.0.1",
    "firebase-admin": "^11.10.1",
    "firebase-functions": "^6.3.2",
    "googleapis": "^128.0.0",
    "mammoth": "^1.9.1",
    "multer": "^1.4.5-lts.1",
    "natural": "^6.12.0",
    "node-fetch": "^3.3.2",
    "openai": "^5.8.2",
    "pdf-parse": "^1.1.1",
    "puppeteer": "^24.14.0"
  },
  "devDependencies": {
    "@types/multer": "^1.4.11",
    "@typescript-eslint/eslint-plugin": "^5.12.0",
    "@typescript-eslint/parser": "^5.12.0",
    "eslint": "^8.9.0",
    "eslint-config-google": "^0.14.0",
    "eslint-plugin-import": "^2.25.4",
    "firebase-functions-test": "^3.1.0",
    "typescript": "^5.8.3"
  },
  "private": true
}
